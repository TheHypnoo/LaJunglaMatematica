<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sergigonzalez.lajunglamatematica">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.LaJunglaMatematica">
        <activity android:name=".AuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />

        <activity android:name=".AnimationLoading" />
        <activity android:name=".menuPrincipal" />
        <activity android:name=".AnimationGame" />
        <activity android:name=".Pruebate" />
        <activity android:name=".Nivel" />
        <activity android:name=".PartidasGuardadas" />
        <activity android:name=".Ranking" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>